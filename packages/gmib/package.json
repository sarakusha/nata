{
  "name": "gmib",
  "version": "3.2.2",
  "description": "Nata-Info gmib tool",
  "private": true,
  "keywords": [
    "nata-info",
    "nibus",
    "sarakusha"
  ],
  "license": "MIT",
  "author": "Andrei Sarakeev <avs@nata-info.ru>",
  "repository": {
    "type": "git",
    "url": "https://github.com/sarakusha/nibus.git",
    "directory": "packages/gmib"
  },
  "bugs": {
    "url": "https://github.com/sarakusha/nibus/issues"
  },
  "scripts": {
    "dev": "cross-env NIBUS_LOG=nibus-all.log DEBUG_COLORS=1 DEBUG=nibus:*,gmib:* electron-webpack dev",
    "dev:mock": "cross-env MOCKED_NIBUS=1 electron-webpack dev",
    "compile": "cross-env NODE_ENV=production ELECTRON_ENABLE_LOGGING=1 electron-webpack",
    "dist": "electron-builder",
    "dist:mac": "yarn dist -c.mac.identity=null",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "dist:linux": "yarn dist -l",
    "dist:win": "yarn dist -w --x64 --ia32",
    "dist:win:dir": "yarn dist -w --x64 --dir",
    "_publish": "yarn compile && yarn release",
    "release:mac": "electron-builder -p always -m --x64 -c.mac.identity=null",
    "release:linux": "electron-builder -p always -l --x64 -c.mac.identity=null",
    "release:win": "electron-builder -p always -w --x64 --ia32 -c.mac.identity=null",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "showMainConfig": "node show-config.js main > maincfg",
    "showRendererConfig": "node show-config.js renderer > ./rendercfg",
    "analyze": "cross-env ANALYZE=1 electron-webpack dev",
    "lint:dev": "eslint --ext .js,.ts,.tsx src/",
    "lint": "eslint -c ../../.eslintrc-prod.js --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint -c ../../.eslintrc-prod.js --ext .js,.ts,.tsx --fix src/",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@mdx-js/react": "^1.6.22",
    "@nibus/cli": "3.2.2",
    "@nibus/core": "3.2.2",
    "@novastar/codec": "^1.0.5",
    "@reduxjs/toolkit": "^1.5.0",
    "@serialport/bindings": "^9.0.1",
    "@types/node": "^15.12.1",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "ajv": "^8.0.5",
    "ansicolor": "^1.1.93",
    "bonjour-hap": "^3.6.2",
    "classnames": "^2.2.6",
    "debug": "^4.3.1",
    "dotenv": "^9.0.2",
    "electron-log": "^4.0.3",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-plugin-import": "^2.18.2",
    "highcharts": "^9.0.0",
    "highcharts-react-official": "^3.0.0",
    "hotkeys-js": "^3.7.3",
    "iput": "^1.1.2",
    "lodash": "^4.17.15",
    "material-ui-chip-input": "^2.0.0-beta.2",
    "node-machine-id": "^1.1.12",
    "notistack": "^1.0.1",
    "pump": "^3.0.0",
    "range-parser": "^1.2.1",
    "rc-slider": "^9.1.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-imask": "^6.0.5",
    "react-redux": "^7.2.2",
    "sanitize-html": "^2.3.2",
    "semver": "^7.3.5",
    "sqlite3": "^5.0.2",
    "suncalc": "^1.8.0",
    "tail": "^2.2.0",
    "tcp-port-used": "^1.0.2",
    "tiny-typed-emitter": "^2.0.3",
    "typeface-roboto": "^1.1.13",
    "typescript": "^4.0.3",
    "usb-detection": "^4.7.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.12.13",
    "@mdx-js/loader": "^1.6.22",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.2",
    "@types/classnames": "^2.2.7",
    "@types/dotenv": "^8.2.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/electron-json-storage": "^4.0.0",
    "@types/jest": "^26.0.22",
    "@types/material-ui": "^0.21.6",
    "@types/pump": "^1.1.1",
    "@types/range-parser": "^1.2.3",
    "@types/rc-slider": "^9.3.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-input-mask": "^3.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/sanitize-html": "^2.3.1",
    "@types/sqlite3": "^3.1.7",
    "@types/suncalc": "^1.8.0",
    "@types/tcp-port-used": "^1.0.0",
    "@types/valid-url": "^1.0.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.0.1",
    "css-loader": "^5.0.0",
    "cz-conventional-changelog": "3.3.0",
    "devtron": "^1.4.0",
    "electron": "12.0.8",
    "electron-builder": "^22.2.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-react-devtools": "^0.5.3",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^4.0.1",
    "env-cmd": "^10.0.1",
    "import-plugin-babel": "npm:babel-plugin-import",
    "react-refresh": "^0.10.0",
    "webpack": "4",
    "webpack-build-notifier": "^2.0.0"
  },
  "workspaces": {
    "nohoist": [
      "usb-detection",
      "@serialport/bindings"
    ]
  },
  "build": {
    "npmRebuild": true,
    "appId": "ru.nata-info.gmib",
    "copyright": "Copyright Â© Nata-Info, 2021",
    "productName": "gmib",
    "publish": "github",
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "category": "Utility",
      "target": "AppImage",
      "desktop": {
        "Name": "${productName}",
        "Terminal": "false"
      }
    },
    "nsis": {
      "artifactName": "${productName}-setup-${version}.${ext}"
    },
    "appImage": {
      "artifactName": "${productName}.${ext}"
    },
    "directories": {
      "buildResources": "resources"
    },
    "extraResources": [
      {
        "from": "./src/extraResources/",
        "to": "extraResources",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
